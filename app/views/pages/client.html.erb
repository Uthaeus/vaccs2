<%= simple_form_for @events do |f| %>
  <div class="jumbotron">
    <%= f.input :title %>
    <%= f.input :start, as: :date, start_year: Date.today.year - 15, end_year: Date.today.year + 12, discard_day: true, order: [:month, :year] %>
    <%= f.input :end, as: :date, start_year: Date.today.year - 15, end_year: Date.today.year + 12, discard_day: true, order: [:month, :year] %>
    <%= f.button :submit %>
  </div>
<% end %>

